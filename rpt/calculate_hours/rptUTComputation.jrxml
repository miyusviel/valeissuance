	<queryString>
		<![CDATA[select eid, nym, log_date, undertime,(salary_base + (st * step_inc_value)) as sal,
((((salary_base + (st * step_inc_value))/30.41666666)/8)/60)*undertime as ut
  from (select e.emp_id as eid, CONCAT(last_name,', ', first_name,' ',COALESCE(middle_name,''), ' ',COALESCE(suffix_name,'')) as nym
,log_date, undertime,
step as st,if(qualified_flag=1,rank,rank-1) as  rnk
from
hours_work h
left join employee e on h.emp_id=e.emp_id
where payroll_period_id=$P{ppid} and (undertime<>0)  order by nym, log_date)
tbl left join salary s on s.rank_value = tbl.rnk  order by nym, log_date]]>
	</queryString>
