	<queryString>
		<![CDATA[SELECT sum(ph.ee), sum(ph.er), sum(epd.pay_comp_amount) FROM emp_pay_details epd INNER JOIN emp_pay ep ON epd.emp_pay_id=ep.emp_pay_id
