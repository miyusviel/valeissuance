	<queryString>
		<![CDATA[SELECT sum(hd.ee), sum(hd.er), sum(epd.pay_comp_amount) FROM emp_pay_details epd INNER JOIN emp_pay ep ON epd.emp_pay_id=ep.emp_pay_id
INNER JOIN pay_component pc ON epd.pay_comp_id=pc.pay_comp_id INNER JOIN pay_classification pcls ON
pc.pay_class_id=pcls.pay_class_id INNER JOIN hdmf hd ON ep.gross_pay BETWEEN hd.range_start AND hd.range_end
WHERE ep.emp_id=$P{emp_id} AND epd.pay_comp_id=10 AND ep.period_id BETWEEN $P{ppid_start} AND $P{ppid_end}]]>
	</queryString>
