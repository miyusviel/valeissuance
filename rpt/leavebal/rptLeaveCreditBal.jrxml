	<queryString>
		<![CDATA[SELECT elcc_id, CONCAT(last_name, ', ', first_name, ' ' , COALESCE(middle_name, ''), COALESCE(suffix_name, '')),
max(if(leave_type_id=2 ,leave_credits,null)) as vacation,
max(if(leave_type_id=2 ,credits_used,null)) as v_used,
max(if(leave_type_id=1 ,leave_credits,null)) as sick,
max(if(leave_type_id=1 ,credits_used,null)) as s_used
FROM emp_leave_credits_closed c INNER JOIN employee e ON c.emp_id=e.emp_id WHERE c.payroll_period_id=$P{id} GROUP BY c.emp_id order by last_name, first_name;]]>
	</queryString>
