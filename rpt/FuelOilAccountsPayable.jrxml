	<queryString>
		<![CDATA[SELECT
   a.acct_id,
   # a.title,
  t.createddate,d.dept_abbreviation,t.ornumber,
  v.description,
    SUM(CASE WHEN f.fuel_id = 1 THEN t.trans_amt ELSE 0 END) AS diesel,
    SUM(CASE WHEN f.fuel_id = 2 THEN t.trans_amt ELSE 0 END) AS gas,
    SUM(CASE WHEN f.fuel_id = 3 THEN t.trans_amt ELSE 0 END) AS oil,
	SUM(CASE WHEN f.fuel_id = 5 THEN t.trans_amt ELSE 0 END) AS others,
   a.code
FROM
    transaction t
INNER JOIN
    accounts a ON t.acct_id = a.acct_id
INNER JOIN
    vehicle v ON v.vehicle_id = t.vehicle_id
INNER JOIN
    fuel f ON f.fuel_id = v.fuel_id
INNER JOIN
    department d ON d.dept_id = v.dept_id
WHERE t.createddate BETWEEN  $P{startdate} AND $P{enddate} and status_id=33
GROUP BY
    #a.acct_id, a.title;
    t.createddate,t.ornumber,v.vehicle_id
order by ornumber]]>
	</queryString>
