	<queryString>
		<![CDATA[SELECT DISTINCT
e.emp_id,
CONCAT(last_name,', ',first_name,' ',middle_name,' ',COALESCE(suffix_name,'')) as nym
FROM employee e INNER JOIN emp_pay ep ON ep.emp_id = e.emp_id WHERE e.emp_id NOT IN (SELECT e.emp_id FROM employee e INNER JOIN emp_pay ep ON ep.emp_id = e.emp_id INNER JOIN emp_pay_details epd ON epd.emp_pay_id = ep.emp_pay_id WHERE epd.pay_comp_id = 7 OR epd.pay_comp_id = 12) AND e.emp_type_id = 1 AND ep.period_id = $P{pp_id} ORDER BY e.last_name]]>
	</queryString>
