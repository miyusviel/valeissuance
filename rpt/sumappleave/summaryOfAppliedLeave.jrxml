	<queryString>
		<![CDATA[SELECT
e.emp_id,
CONCAT(last_name,', ',first_name,' ',middle_name,' ',COALESCE(suffix_name,'')) as nym,
lt.leave_type_desc,
la.start_date_time,
la.end_date_time,
la.leave_days,
la.credits_used,
la.reason,
la.leave_app_id AS lai,
(SELECT CONCAT(last_name,', ',first_name,' ',middle_name,' ',COALESCE(suffix_name,'')) FROM employee e, leave_app la WHERE la.r_approved_by = e.emp_id AND la.leave_app_id = lai) AS rec_app,
(SELECT CONCAT(last_name,', ',first_name,' ',middle_name,' ',COALESCE(suffix_name,'')) FROM employee e, leave_app la WHERE la.approved_by = e.emp_id AND la.leave_app_id = lai) AS app FROM employee e INNER JOIN leave_app la ON e.emp_id = la.emp_id INNER JOIN leave_type lt ON la.leave_type_id = lt.leave_type_id WHERE e.emp_type_id=1 AND la.status_id = 2 AND la.trans_date BETWEEN $P{START_DATE} AND $P{END_DATE} ORDER BY nym]]>
