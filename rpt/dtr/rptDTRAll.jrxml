	<queryString>
		<![CDATA[Select emp_id, CONCAT(last_name,', ',first_name,' ',COALESCE(middle_name,''), ' ', COALESCE(suffix_name,'')) as nym, dn.dept_name  from employee e
left join section s on e.sec_id=s.sec_id
left join division d on s.div_id=d.div_id
left join department dn on d.dept_id=dn.dept_id
where emp_type_id=$P{etype} and employer_id=$P{eto} and d.dept_id=$P{did}
and emp_id IN (select emp_id from time_log_final where payroll_period_id=$P{ppid}) group by e.emp_id order by last_name
]]>
