	<queryString>
		<![CDATA[SELECT SUM($P{basic_salary} / 30.41666666 / 8 / 60 * eod.ot_hours * pc.multiplier) AS total FROM emp_ot_details eod INNER JOIN pay_component pc ON pc.pay_comp_id = eod.pay_comp_id WHERE eod.emp_id = $P{emp_id} AND eod.ot_date BETWEEN $P{start_date} AND $P{end_date}]]>
	</queryString>
	<field name="total" class="java.lang.Double">
