	<queryString>
		<![CDATA[SELECT DISTINCT e.emp_id, CONCAT(last_name,', ',first_name,' ',middle_name,' ',COALESCE(suffix_name,'')) as nym, epd.pay_comp_amount  FROM employee e INNER JOIN emp_pay ep ON ep.emp_id = e.emp_id INNER JOIN emp_pay_details epd ON epd.emp_pay_id = ep.emp_pay_id INNER JOIN emp_ot_details eod ON eod.emp_id = e.emp_id WHERE ep.period_id = $P{PAYROLL_ID} AND epd.pay_comp_id = 1 AND eod.ot_date BETWEEN $P{START_DATE} AND $P{END_DATE} ORDER BY nym]]>
